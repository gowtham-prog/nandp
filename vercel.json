{
    "builds" :[{
        "src" : "notes_password/wsgi.py",
        "use" : "@vercel/python",
        "config" : { "maxLambdaSize" : "15mb", "runtime" : "python3.9"}
    }],
    "routes" : [
        {
            "src" : "/(.*)",
            "dest" : "notes_password/wsgi.py"
        }
    ]
}
